@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
        height: 400px;
        width: 100%;
    }
   
</style>
<div style="padding-top:100px; z-index: -100;">
   

    <div id="map" >
       
    </div>
</div>




<script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 19.4394894, lng: -99.25809249999999},
          zoom: 13
        });
      }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD4b6Dh0VHgxcPywJ6cxj1uvBMo5Ne03nA&callback=initMap"
        async defer></script>

